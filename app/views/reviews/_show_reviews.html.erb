 <% if reviews.present?%>
        <%reviews.each do |r|%>
  Review</br>
  <%=r.content%></br>
  Rating</br>
<div class="fixed" data-score=<%=r.rating%>></div>
    <%= social_share_button_tag(r.content,desc:"#{r.content}",:image=>asset_path("hotel.jpeg", image)) %>

        <%end%>

<%end%>
<script type="text/javascript">
$('.fixed').raty({
  readOnly : true,
  score: function() {
    return $(this).attr('data-score');
  },
      path:  '/assets/'

});</script>