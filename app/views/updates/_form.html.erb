<%= simple_form_for(@update) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :topic %>
    <%= f.input :details %>
    <%#= f.input :tag_list%>
    <%= f.input :tag_list, :label => "News Tag", :"data-delimiter" => ' ' ,:url => autocomplete_tag_name_updates_path, :as => :autocomplete%>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
